        "@fortawesome/react-fontawesome": "0.2.2",
        "@types/react-slick": "0.23.13",
        "classnames": "2.5.1",
        "fontawesome": "5.6.3",
        "framer-motion": "11.3.30",
        "graphql": "16.9.0",
        "next": "14.1.4",
        "react": "^18",
        "react-copy-to-clipboard": "5.1.0",
        "react-dom": "^18",
        "react-icons": "5.3.0",
        "react-multi-carousel": "2.8.5",
