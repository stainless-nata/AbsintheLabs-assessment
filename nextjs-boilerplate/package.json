        "eslint-plugin-prettier": "5.1.3",
        "husky": "9.0.11",
        "lint-staged": "15.2.2",
        "postcss": "^8",
        "prettier": "3.2.5",
        "prettier-plugin-tailwindcss": "0.5.13",
        "stylelint": "16.3.1",
        "stylelint-config-standard": "36.0.0",
